{
  "name": "@sentre/utility",
  "version": "1.0.2",
  "main": "dist/app/index.js",
  "scripts": {
    "start": "nodemon app/index.ts",
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./dist",
    "predeploy": "typedoc --excludePrivate --excludeProtected --includeVersion ./app/index.ts",
    "deploy": "echo utility.sentre.io >> ./docs/CNAME && gh-pages -d docs",
    "test": "npm run build && mocha --require ts-node/register -t 120000 lib-test/**/*.test.ts"
  },
  "author": "Tu Phan <tuphan@descartes.network>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DescartesNetwork/sen-utility.git"
  },
  "bugs": {
    "url": "https://github.com/DescartesNetwork/sen-utility/issues"
  },
  "homepage": "https://utility.sentre.io",
  "keywords": [
    "solana",
    "sentre",
    "utility"
  ],
  "dependencies": {
    "@project-serum/anchor": "^0.24.2",
    "bs58": "^5.0.0",
    "js-sha3": "^0.8.0"
  },
  "devDependencies": {
    "@types/bn.js": "^5.1.0",
    "@types/chai": "^4.3.0",
    "@types/mocha": "^9.0.0",
    "@types/node": "^17.0.34",
    "chai": "^4.3.4",
    "gh-pages": "^4.0.0",
    "mocha": "^9.0.3",
    "ts-mocha": "^8.0.0",
    "ts-node": "^7.0.1",
    "typedoc": "^0.22.15",
    "typescript": "^4.3.5"
  }
}
